{
  "name": "blog",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "build": "rollup -c --bundleConfigAsCjs",
    "transpile": "tsc",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "clean:install": "rm -rf node_modules && rm -rf package-lock.json && npm i",
    "clean:build": "rm -rf dist && rm -rf build && npm run build",
    "clean:install:build": "npm run clean:install && npm run clean:build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.11",
    "@types/node-fetch": "^2.6.2",
    "@types/prettier": "2.6.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "esbuild": "^0.15.16",
    "eslint": "^8.34.0",
    "jest": "^27.5.1",
    "rollup": "^3.17.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "~4.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.279.0",
    "@aws-sdk/client-secrets-manager": "^3.279.0",
    "@aws-sdk/util-dynamodb": "^3.279.0",
    "aws-cdk": "^2.53.0",
    "aws-cdk-lib": "2.40.0",
    "constructs": "^10.0.0",
    "node-fetch": "^2.6.9",
    "openai": "^3.1.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2",
    "zod": "3.20.2"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "clearMocks": true,
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
